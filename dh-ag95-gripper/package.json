{
    "name": "dh-ag95-gripper",
    "version": "0.0.0",
    "scripts": {
        "start": "npm run start --prefix dh-ag95-gripper-frontend",
        "install-frontend": "npm install --prefix dh-ag95-gripper-frontend",
        "install-backend": "npm install --prefix dh-ag95-gripper-backend",
        "lint": "npm run lint --prefix dh-ag95-gripper-frontend",
        "postinstall": "npm run install-frontend && npm run install-backend",
        "prebuild": "npm run clean-dist && npm run clean-target && npm run validate-manifest",
        "build-arm": "export ARCH=linux/arm64 && npm run build",
        "build": "npm run build-backend && npm run build-frontend",
        "postbuild": "npm run package",
        "build-backend": "npm run build --prefix dh-ag95-gripper-backend",
        "build-frontend": "npm run build --prefix dh-ag95-gripper-frontend",
        "prepackage": "mkdir dist && npm run copy-backend && npm run copy-frontend && npm run copy-manifest && npm run copy-license && npm run write-sbom",
        "package": "package-urcap dist target",
        "copy-backend": "ncp 'dh-ag95-gripper-backend/dist' 'dist/dh-ag95-gripper-backend'",
        "copy-frontend": "ncp 'dh-ag95-gripper-frontend/dist' 'dist'",
        "copy-manifest": "ncp 'manifest.yaml' 'dist/manifest.yaml'",
        "write-sbom": "npm run write-sbom-frontend",
        "write-sbom-frontend": "write-ur-packages ./dh-ag95-gripper-frontend/package-lock.json dist",
        "copy-license": "ncp 'LICENSE' 'dist/LICENSE'",
        "clean-dist": "rimraf dist",
        "clean-target": "rimraf target",
        "install-urcap": "install-urcap 'target/dh-ag95-gripper*.urcapx' './manifest-spec-19.6.14.json'",
        "delete-urcap": "delete-urcap foundry-robotics dh-ag95-gripper",
        "validate-manifest": "validate-manifest 'manifest.yaml' './manifest-spec-19.6.14.json'"
    },
    "private": true,
    "devDependencies": {
        "@universal-robots/urcap-utils": "1.6.4",
        "ncp": "2.0.0",
        "rimraf": "^5.0.7"
    }
}
